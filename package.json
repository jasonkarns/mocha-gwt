{
  "name": "mocha-gwt",
  "version": "0.2.1",
  "description": "Given When Then for Mocha",
  "main": "index.js",
  "browser": "lib/mocha-gwt.coffee",
  "config": {
    "build_file": "dist/mocha-gwt.js"
  },
  "scripts": {
    "prepublish": "npm test",
    "test": "npm run unit && npm run e2e",
    "unit": "mocha test/unit/",
    "e2e": "coffee runtest.coffee",
    "build": "browserify . --extension=.coffee -o $npm_package_config_build_file"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheLudd/mocha-gwt"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "mocha": ">=1.20.1"
  },
  "devDependencies": {
    "bluebird": "^2.3.5",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "chai": "^3.0.0",
    "coffeeify": "^1.1.0",
    "mocha": "^2.0.0",
    "nodemon": "^1.4.1"
  },
  "dependencies": {
    "assertion-error": "^1.0.0",
    "coffee-script": "^1.7.1",
    "ramda": "^0.17.1"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "mocha": "global:Mocha"
  }
}
